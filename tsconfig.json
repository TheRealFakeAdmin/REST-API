{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": true,
      "lib": ["ES2015"],
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }
  